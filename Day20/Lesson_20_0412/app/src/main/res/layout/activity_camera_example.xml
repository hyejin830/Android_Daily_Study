<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".example02_camera.CameraExampleActivity">

    <TextView
        android:id="@+id/tv_view_data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/camera_example_data"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/tv_view_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/camera_example_address"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btn_do_scan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/camera_example_scan"
        android:textSize="30sp" />

</LinearLayout>